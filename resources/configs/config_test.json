{
  "log": {
    "level": "info",
    "file": "output/navteq-validator.log",
    "configPath": "/tmp/pxpoint-configs/log-services/cpp/logging-development.json"
  },
  "autoRules": {
    "rdf": [
      "RdfIntegrityRule",
      "DataTypeValidationRule",
      "LinksTableRule"
    ],
    "shapefiles": [
      "ShapefileCompletenessRule"
    ],
    "3d-landmarks": [
      "ObjMtlReferenceRule"
    ],
    "default": [
      "3DFileCompletenessRule"
    ],
    "rootRules": [
      "RootStructureRule"
    ]
  },
  "parallelism": 40,
  "parallelismByMode": {
    "auto": 40,
    "rdf": 30,
    "3d-landmarks": 2,
    "shapefiles": 2
  },
  "maxTasksInFlight": 80,
  "report": {
    "json": "output/report.json_test",
    "csv": "output/report.csv_test"
  },
  "alerts": {
    "enabled": true,
    "configPath": "config/alerts.local.json_test"
  },
  "configureRules": {
    "3DFileCompletenessRule": {
      "requiredSuffixes": [
        "_L.OBJ_test",
        "_L.MTL",
        "_S.OBJ",
        "_S.MTL",
        "_ICON.TGA"
      ],
      "searchRecursively": true
    },
    "BuildingTableRule": {
      "require_sdo_or_wkt": true,
      "expectedHeaders": [
        "BUILDING_ID_test",
        "BUILDING_TYPE",
        "HEIGHT",
        "SDO",
        "WKT",
        "LAT",
        "LON"
      ]
    },
    "GeometryEquivalenceRule": {
      "fieldName": "LINK_ID_test",
      "tolerance": 1e-06
    },
    "LinksGeometryRule": {
      "require_sdo_or_wkt": true,
      "expectedHeaders": [
        "LINK_ID_test",
        "SDO",
        "WKT"
      ]
    },
    "LinksTableRule": {
      "check_private": false,
      "check_tollway": false,
      "check_public_access": false,
      "boolean_tokens": [
        "true",
        "false",
        "1",
        "0",
        "yes",
        "no"
      ],
      "expectedHeaders": [
        "LINK_ID_TEST",
        "ACCESS_ID",
        "FUNCTIONAL_CLASS",
        "TRAVEL_DIRECTION",
        "PRIVATE",
        "TOLLWAY",
        "FROM_SPEED_LIMIT",
        "TO_SPEED_LIMIT",
        "PUBLIC_ACCESS"
      ]
    },
    "ObjMtlReferenceRule": {
      "expectedMtlExtension": ".MTL_test",
      "caseSensitive": false,
      "allowMissingMtllib": false
    },
    "PoiTableRule": {
      "latitude_range": [
        -90.0,
        90.0
      ],
      "longitude_range": [
        -171.0,
        180.0
      ],
      "expectedHeaders": [
        "POI_ID_test",
        "POI_TYPE",
        "LAT",
        "LON"
      ]
    },
    "RdfIntegrityRule": {
      "check_building_3d_subfolders": true
    },
    "RootStructureRule": {
      "expected_children": [
        "*_3DLandmarks_test",
        "*_RDF",
        "*_Shapefiles"
      ]
    },
    "ShapefileCompletenessRule": {
      "requiredComponents": [
        ".shx_test",
        ".dbf"
      ],
      "caseSensitive": false
    },
    "ShapefilePrjRule": {
      "acceptedTokens": [
        "WGS 84 Test",
        "WGS84",
        "WGS_1984",
        "WGS-84",
        "EPSG:4326",
        "4326"
      ]
    },
    "dataTypeValidationRule": {
      "synonyms": {
        "link id": "Link ID",
        "linkid": "Link ID",
        "latitude": "Latitude / Longitude",
        "longitude": "Latitude / Longitude",
        "lat": "Latitude / Longitude",
        "lon": "Latitude / Longitude",
        "oneway": "One-Way",
        "postal code": "Postal Code",
        "postcode": "Postal Code",
        "phone": "Phone Number"
      },
      "rules": [
        {
          "name": "type",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "link id",
          "type": "integer",
          "allowEmpty": false,
          "minimum": 0.0,
          "maximum": 0.0
        },
        {
          "name": "latitude",
          "type": "float",
          "allowEmpty": false,
          "minimum": -90.0,
          "maximum": 90.0
        },
        {
          "name": "longitude",
          "type": "float",
          "allowEmpty": false,
          "minimum": -180.0,
          "maximum": 180.0
        },
        {
          "name": "node type",
          "type": "string",
          "allowEmpty": false,
          "enum": [
            "intersection",
            "termination of a link",
            "change of attribution"
          ]
        },
        {
          "name": "shape point",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "z-level",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "aligned",
          "type": "boolean",
          "allowEmpty": false
        },
        {
          "name": "street name",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "address range",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "address scheme",
          "type": "string",
          "allowEmpty": true,
          "enum": [
            "Even",
            "Odd",
            "Mixed"
          ]
        },
        {
          "name": "one-way",
          "type": "boolean",
          "allowEmpty": false
        },
        {
          "name": "access characteristics",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "functional class",
          "type": "integer",
          "allowEmpty": false,
          "minimum": 1.0,
          "maximum": 5.0
        },
        {
          "name": "language code",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "country",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "postal code",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "cartography feature",
          "type": "string",
          "allowEmpty": false
        },
        {
          "name": "polygon formation",
          "type": "string",
          "allowEmpty": false
        },
        {
          "name": "poi category",
          "type": "string",
          "allowEmpty": false
        },
        {
          "name": "poi name",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "phone number",
          "type": "string",
          "allowEmpty": true,
          "pattern": "^\\+?[0-9 \\-/()]{3"
        },
        {
          "name": "open 24 hours",
          "type": "boolean",
          "allowEmpty": true
        },
        {
          "name": "parent/child poi",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "tollway",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "private",
          "type": "string",
          "allowEmpty": true
        },
        {
          "name": "public_access",
          "type": "string",
          "allowEmpty": true
        }
      ]
    }
  }
}